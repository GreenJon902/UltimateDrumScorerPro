#:kivy 2.1.0

#:set text_input_radius mm(5), mm(5)
#:set padding mm(5), mm(5), mm(5), mm(5)

<TextScoreSectionEditor>:
    text: "Text Editor"
    text_input: textInput

    BoxLayout:
        padding: padding
        RoundedTextInput:
            id: textInput

            radius: text_input_radius
